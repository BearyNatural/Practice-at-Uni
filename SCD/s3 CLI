paste credentials
aws s3 ls

--Create an s3 bucket using the aws cli
aws s3 mb s3://[bucketname]

cd SCD

--Upload a couple of objects to the bucket
aws s3 cp testfile.txt s3://bucketname/testfile.txt
aws s3 cp folder/ s3://bucketname/folder/ --recursive

--list the objects in your bucket
aws s3 ls s3://bucketname/
aws s3 ls s3://bucketname/folder/

--retrieve the object metadata
aws s3api head-object --bucket bucketname --key testfile.txt

--download the object from the buckeet
aws s3api get-object --bucket bucketname --key testfile.txt testfile4.txt

--delete the objects from the bucket
aws s3api delete-object --bucket bucketname --key testfile.txt

--debug the object from the bucket
aws s3 cp testfile.txt s3://emilytheneomastiff1234567890//testfile.txt --debug

